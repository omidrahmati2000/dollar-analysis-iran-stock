    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "d3": "^7.8.4",
    "date-fns": "^2.29.3",
    "lightweight-charts": "^4.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "plotly.js": "^2.20.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.2",
    "react-plotly.js": "^2.6.0",
    "react-query": "^3.39.3",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.2",
    "react-virtualized": "^9.22.3",
    "recharts": "^2.5.0",
    "socket.io-client": "^4.6.1",
    "styled-components": "^5.3.9",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*'",
    "test:component": "cypress run --component",
    "test:ui": "cypress run --spec 'cypress/e2e/02-advanced-charts-ui.cy.js'",
    "test:logic": "cypress run --spec 'cypress/e2e/03-indicators-logic.cy.js'",
    "test:performance": "cypress run --spec 'cypress/e2e/06-performance-tests.cy.js'",
    "test:integration": "cypress run --spec 'cypress/e2e/05-integration-flows.cy.js'",
    "test:all": "npm run test && npm run test:e2e",
    "test:headless": "cypress run --headless --browser electron",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cypress": "^14.5.4",
    "cypress-real-events": "^1.14.0",
    "cypress-wait-until": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "proxy": "http://localhost:8000"
}
